{
    "compilerOptions": {
      "skipLibCheck": true,
      "target": "es2022",
      "esModuleInterop": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "moduleDetection": "force",
      "isolatedModules": true,
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "module": "NodeNext",
      "moduleResolution": "nodenext"
    }
  }